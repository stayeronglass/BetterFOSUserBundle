{
    "name": "stayeronglass/better-fosuser-bundle",
    "type": "symfony-bundle",
    "description": "Better FOSUserBundle",
    "keywords": [
        "User management"
    ],
    "homepage": "http://friendsofsymfony.github.com",
    "license": "MIT",
    "authors": [
        {
            "name": "stayeronglass"
        },
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        },
        {
            "name": "Thibault Duplessis"
        },
        {
            "name": "FriendsOfSymfony Community",
            "homepage": "https://github.com/friendsofsymfony/FOSUserBundle/contributors"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-dom": "*",
        "ext-json": "*",
        "symfony/config": "^7.0",
        "symfony/dependency-injection":  "^7.0",
        "symfony/event-dispatcher":  "^7.0",
        "symfony/event-dispatcher-contracts":  "^7.0",
        "symfony/form":  "^7.0",
        "symfony/framework-bundle":  "^7.0",
        "symfony/http-foundation":  "^7.0",
        "symfony/http-kernel":  "^7.0",
        "symfony/options-resolver":  "^7.0",
        "symfony/routing":  "^7.0",
        "symfony/security-bundle":  "^7.0",
        "symfony/security-core":  "^7.0",
        "symfony/translation":  "^7.0",
        "symfony/twig-bundle":  "^7.0",
        "symfony/validator":  "^7.0",
        "twig/twig": "^2.4 || ^3.0"
    },
    "conflict": {
        "doctrine/doctrine-bundle": "<1.3",
        "doctrine/persistence": "<1.3",
        "symfony/doctrine-bridge": "<4.4"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.3 || ^2",
        "friendsofphp/php-cs-fixer": "^3.0.2, !=3.5.0",
        "swiftmailer/swiftmailer":  "^7.0",
        "symfony/console":  "^7.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/yaml":  "^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "FOS\\UserBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "FOS\\UserBundle\\Tests\\": "/Tests/"
        }
    },
    "support": {
        "docs": "https://symfony.com/doc/master/bundles/FOSUserBundle/index.html"
    }
}
